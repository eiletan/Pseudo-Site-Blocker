{
  "name": "Pseudo Site blocker",
  "version": "3.0",
  "description": "Annoy yourself into being productive",
  "permissions": ["storage","tabs","activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon":{
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  },
  "manifest_version": 2
}