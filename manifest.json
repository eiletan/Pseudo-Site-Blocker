{
  "name": "Pseudo Site blocker",
  "version": "2.0",
  "description": "Boost your productivity by pseudo-blocking websites frequently visited during procrastination",
  "permissions": ["storage","tabs","activeTab", "http://*/", "https://*/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon":{
      "128": "bbt128.png"
    }
  },
  "icons": {
    "128": "bbt128.png"
  },
  "manifest_version": 2
}